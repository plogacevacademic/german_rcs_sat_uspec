title = {{Oculomotor behavior and perceptual strategies in complex tasks}},
url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?db=pubmed\&cmd=Retrieve\&dopt=AbstractPlus\&list\_uids=11718797},
volume = {41},
year = {2001}
}
