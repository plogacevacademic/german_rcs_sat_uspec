@article{Carpenter:2007p1556,
abstract = {When saccades are evoked by suddenly pre- sented visual stimuli, the stochastic distribution of their reaction times is typically recinormal, in conformity with the LATER model of decision-making, some- times with an additional sub-population of early responses. In the real world, saccades are more often spontaneous responses to static features of the visual world; nevertheless, the time between the end of one saccade and the start of the next may be regarded as a reaction time to the new image that is presented to the retina. The distribution of these times is qualitatively similar to that of evoked saccades, but typically with a longer median and more of the early responses. Here, we analyse the statistical distributions of Wxation times from a large database of spontaneous saccades made while reading and show that the distributions are altered in characteristic ways by particular features of the current Wxation: likely familiarity of the currently Wxated word, and its proximity to the preceding Wxa- tion. These alterations are of the kind predicted by LATER: familiarity appears to inXuence the mean rate at which the decision signal approaches completion, whereas proximity to the previous Wxation, presumably because it provides partial prior information about the upcoming word, appears to increase prior probability. We conclude that spontaneous saccades may be suc- cessfully described by the same decision-making model that can be used for evoked ones},
author = {Carpenter, R and McDonald, Scott A},
file = {:Users/pavel/Dropbox/Papers/Carpenter, McDonald/Carpenter, McDonald - 2007 - Experimental Brain Research.pdf:pdf},
journal = {Experimental Brain Research},
keywords = {distributions},
