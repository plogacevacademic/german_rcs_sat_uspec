journal = {Cognitive Psychology},
keywords = {eyemindhypothesis,eyemovements,reanalysis,regressions,sentenceprocessing},
month = apr,
number = {2},
pages = {178--210},
title = {{Making and correcting errors during sentence comprehension: Eye movements in the analysis of structurally ambiguous sentences}},
url = {http://linkinghub.elsevier.com/retrieve/pii/0010028582900081},
volume = {14},
year = {1982}
}
