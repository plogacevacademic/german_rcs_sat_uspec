keywords = { flamewar, sentencecomprehension,aphasia},
month = feb,
number = {2},
pages = {158--184},
pmid = {11254256},
title = {{Agrammatic Broca's aphasia is not associated with a single pattern of comprehension performance}},
volume = {76},
year = {2001}
}
