journal = {Frontiers in Neuroinformatics},
keywords = {neuroinformatics},
mendeley-tags = {neuroinformatics},
month = jan,
pages = {33},
title = {{COINS: An Innovative Informatics and Neuroimaging Tool Suite Built for Large Heterogeneous Datasets}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3250631\&tool=pmcentrez\&rendertype=abstract},
volume = {5},
year = {2011}
}
