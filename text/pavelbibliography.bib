title = {{An ERP study of P600 effects elicited by semantic anomalies}},
url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?db=pubmed\&cmd=Retrieve\&dopt=AbstractPlus\&list\_uids=15653297},
volume = {22},
year = {2005}
}
