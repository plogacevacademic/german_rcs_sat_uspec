issn = {1872-6240},
journal = {Brain Research},
keywords = {Asian Continental Ancestry Group,Brain,Brain: physiology,Comprehension,Comprehension: physiology,Computer-Assisted,ERP,Electroencephalography,Evoked Potentials,Evoked Potentials: physiology,Female,Humans,Language,Local reference,Long-distance reference,Male,P300,P600,Pattern Recognition,Reflexive pronoun,Semantics,Sentence comprehension,Signal Processing,Visual,Visual: physiology,Young Adult},
month = may,
number = {1981},
