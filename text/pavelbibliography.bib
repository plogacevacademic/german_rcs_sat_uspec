journal = {Lancet},
keywords = {clinical trials,private funding},
mendeley-tags = {clinical trials,private funding},
month = dec,
number = {9808},
pages = {1995--6},
title = {{Access to data in industry-sponsored trials.}},
url = {http://www.thelancet.com/journals/a/article/PIIS0140-6736(11)61871-0/fulltext},
volume = {378},
year = {2011}
}
