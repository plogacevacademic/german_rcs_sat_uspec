pages = {164--203},
pmid = {12948517},
title = {{The misinterpretation of noncanonical sentences}},
url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?db=pubmed\&cmd=Retrieve\&dopt=AbstractPlus\&list\_uids=12948517},
volume = {47},
year = {2003}
