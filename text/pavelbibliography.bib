journal = {Neuroscience letters},
keywords = {Adult,Analysis of Variance,Electroencephalography,Electroencephalography: methods,Evoked Potentials,Evoked Potentials: physiology,Female,Humans,Male,Semantics,Vocabulary},
month = may,
number = {1-2},
pages = {7--12},
pmid = {16503377},
title = {{Does the frequency of the antecedent noun affect the resolution of pronominal anaphors? An ERP study.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16503377},
volume = {400},
year = {2006}
}
