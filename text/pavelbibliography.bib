journal = {Quarterly journal of experimental psychology},
keywords = {Analysis of Variance,Cognition,Cognition: physiology,Comprehension,Comprehension: physiology,Humans,Language Arts,Psycholinguistics,Reaction Time,Reaction Time: physiology,Reading,Semantics,Task Performance and Analysis},
month = dec,
