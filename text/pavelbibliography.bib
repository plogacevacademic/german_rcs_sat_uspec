institution = {Department of Cognitive and Linguistic Sciences, Brown University, Providence, RI 02912, USA. julie\_sedivy@brown.edu},
journal = {Cognition},
keywords = {adult,attention,color perception,discrimination learning,eye movements,female,humans,male,pattern recognition,psychomotor performance,reading,semantics,speech perception,visual},
number = {2},
pages = {109--147},
pmid = {10444906},
publisher = {Elsevier},
title = {{Achieving incremental semantic interpretation through contextual representation.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/10444906},
volume = {71},
