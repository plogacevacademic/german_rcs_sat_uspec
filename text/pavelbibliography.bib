@article{SalvucciAnderson2001,
abstract = {This article describes and evaluates a class of methods for performing
automated analysis of eye-movement protocols. Although eye movements
have become increasingly popular as a tool for investigating user
behavior, they can be extremely difficult and tedious to analyze.
In this article we propose an approach to automating eye-movement
protocol analysis by means of tracing--relating observed eye movements
to the sequential predictions of a process model. We present three
tracing methods that provide fast and robust analysis and alleviate
