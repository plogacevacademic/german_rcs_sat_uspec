title = {{Diagnosis and repair of negative polarity constructions in the light of symbolic resonance analysis}},
url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?db=pubmed\&cmd=Retrieve\&dopt=AbstractPlus\&list\_uids=15975647},
volume = {96},
year = {2006}
