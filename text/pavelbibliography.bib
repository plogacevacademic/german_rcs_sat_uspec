@article{Clifton1997a,
abstract = {Two questionnaire studies and two experiments investigated the processing of the pronouns her and him/his. The questionnaires and the first experiment searched for effects of lexical preferences in resolving the temporary syntactic category ambiguity of the word her. Contingent frequency based on a verb's preference for a human vs an inanimate direct object demonstrably affected the final interpretation of an ambiguous her following the verb in an off-line questionnaire study. However, it did not influence on-line processing times for SPEC versus NP uses of her. This result is surprising from the perspective of current lexically based models of ambiguity resolution. Experiment 2 addressed how potential antecedents for the unambiguous pronouns him and his are identified. Both Experiment I and Experiment 2 showed that NP uses of personal pronouns like her and him could be processed more easily than SPEC uses of her and his. Experiment 2 showed that this difference largely reflected differences in difficulty of choosing an antecedent for the pronoun. To account for the processing of her, we proposed that the parser initially builds an underspecified syntactic representation, fixing further details and disambiguating the category of her as subsequent information becomes available. Comprehension proceeds by attempting to find an antecedent for the personal pronoun, with the preferred candidate a syntactically available within-sentence antecedent if one exists. (C) 1997 Academic Press.},
annote = {- resolution of personal pronouns as np "he saw her" vs. spec "he saw her cat"
- two questionnaires: some verbs are biased towards either possibility
- experiment 1: verb bias does not affect difficulty of resolution, but spec is more difficult than np; perhaps looking for legal antecedent within clause because possible but none is found in spec case
- experiment 2: manipulation number of subject np to vary if legal antecedent present or not; further manipulation his/him (overt marking spec or np); effect of subject number only for spec (contra badecker \& straub 1994)
- conclusion: no early influence of verb properties on pronoun resolution, but overall advantage for np pronouns that disappears when the sentence subject agrees with the spec pronoun "his"
- the parser immediately attempts to find a syntactically legal antecedent in the same sentence},
author = {Clifton, C and Kennison, S M and Albrecht, J E},
journal = {Journal of Memory and Language},
keywords = { Assignment, Eye-Movements, Probabilistic Constraints, Sentences,Syntactic Ambiguity Resolution,double?},
mendeley-tags = {double?},
